nd-color:#cccccc;background-image:url("http://www.smooththemes.com/api/hotec/hotec-wp/assets/images/patterns/pattern1.png");background-repeat:repeat;}.slider-outer-wrapper{overflow:hidden;}';jQuery('style#boxed_default').append(html);jQuery('.body-wrapper').addClass('boxed-mode');jQuery(window).resize();}else{jQuery('.body-wrapper').removeClass('boxed-mode');jQuery('style#boxed_default').empty();jQuery(window).resize();}});jQuery('.styler_colors a').click(function(e){e.preventDefault();jQuery(this).parent().find('a').removeClass('active');jQuery(this).addClass('active');var name=jQuery(this).attr('name');if(name=='brown'){jQuery('#styler_switch').attr('href','');}else{jQuery('#styler_switch').attr('href','http://www.smooththemes.com/api/hotec/hotec-wp/assets/css/skins/'+name+'.css');}});jQuery('.styler_bgcolors a').click(function(e){e.preventDefault();jQuery(this).parent().find('a').removeClass('active');jQuery(this).addClass('active');var bg=jQuery(this).css('backgroundColor');jQuery('body').css('backgroundColor',bg).css('backgroundImage','none')});jQuery('.styler_patterns a').click(function(e){e.preventDefault();jQuery(this).parent().find('a').removeClass('active');jQuery(this).addClass('active');var name=jQuery(this).attr('name');if(jQuery(this).hasClass('pattern')){jQuery('body').css('background','url(http://www.smooththemes.com/api/hotec/hotec-wp/assets/images/patterns/'+name+'.png) repeat center center scroll');jQuery('body').css('background-size','auto');}else{jQuery('body').css('background','url(http://www.smooththemes.com/api/hotec/hotec-wp/assets/images/patterns/'+name+'.jpg) no-repeat center center fixed');jQuery('body').css('background-size','cover');}});});</script></body></html>